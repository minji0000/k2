<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %><%@ taglib prefix="fmt" uri="http://java.sun.com/jsp/jstl/fmt" %><%@ page contentType="text/html;charset=UTF-8" language="java" %><html><head>    <title>k2 Web 게시판 </title></head><body><div>   <H2>하이하이</H2>    <table>        <thead>        <tr>            <th>글번호</th>            <th>글제목</th>            <th>작성자</th>            <th>작성일</th>            <th>수정일</th>        </tr>        </thead>        <c:forEach items="${list}" var="b">            <tr>                <td>${b.id}</td>                <td onclick="location.href ='/board/showOne/${b.id}'">${b.title}</td>                <td>${nicknameMap[b.writerId]}</td>                <td><fmt:formatDate value="${b.writtenDate.time}" pattern="20yy년 MM월 dd일 HH:mm:ss"/></td>                <td><fmt:formatDate value="${b.updatedDate.time}" pattern="20yy년 MM월 dd일 HH:mm:ss"/></td>            </tr>        </c:forEach>    </table></div></body></html>