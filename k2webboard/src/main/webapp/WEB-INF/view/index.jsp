<%@ page language="java" contentType="text/html; charset=UTF-8"         pageEncoding="UTF-8" %><!DOCTYPE html><html><head>    <title> k2 Web LogIn </title>    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"          integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous"></head><body><center>    <div class="mt-5">        <h2>로그인 해주세요</h2>        <form action="user/login" method="post">            <div class="mb-3 mt-5">                <label class="form-label">아이디</label>                <input class="form-control w-100" type="text" name="username"/>            </div>            <div class="mb-3">                <label class="form-label">비밀번호</label>                <input class="form-control " type="password" name="password"/>            </div>            <button class="btn btn-outline-primary btn-sm mb-3" type="submit">로그인</button>        </form>        <button class="btn btn-outline-primary btn-sm"                onclick="location.href='/user/register'">회원가입        </button>    </div>    <div class="container">    </div></center></body></html>