<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="mapper.ReplyMapper">    <insert id="insert" parameterType="ReplyDTO">        INSERT INTO reply(board_id, content, writer_id)        VALUES (#{boardId}, #{content}, #{writerId})    </insert>    <select id = "selectAll" parameterType = "int" resultType = "ReplyDTO">        SELECT * FROM reply WHERE board_id = #{boardId}    </select></mapper>